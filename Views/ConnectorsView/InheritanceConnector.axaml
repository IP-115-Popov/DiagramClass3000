<Styles xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:controls="using:DiagramClass.Views.ConnectorsView">
  <Design.PreviewWith>
    <controls:InheritanceConnector EndPointCastom="80,80" StartPointCastom="90,90"/>
  </Design.PreviewWith>

  <Style Selector="controls|InheritanceConnector">
    <!-- Set Defaults -->
    <Setter Property="Template">
      <ControlTemplate>
			<Line   StartPoint="{TemplateBinding StartPointCastom}"
					EndPoint="{TemplateBinding EndPointCastom}"
					StrokeThickness="5"
					Stroke="Black"
				/>
      </ControlTemplate>
    </Setter>
  </Style>
</Styles>
