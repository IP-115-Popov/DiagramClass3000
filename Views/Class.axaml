<Styles xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:controls="using:DiagramClass.Views">
  <Design.PreviewWith>
    <controls:Class MyTupeText="Class" TupeName="LOL" MyExtension="public"/>
  </Design.PreviewWith>

  <Style Selector="controls|Class">
    <!-- Set Defaults -->
    <Setter Property="Template">
      <ControlTemplate>
		  <Canvas Height="128" Width="128" Background="Transparent">
			  <Rectangle Height="118" Width="118" Margin="5,5" Stroke="Black" StrokeThickness="2"/>	  
			  <Ellipse Width="10" Height="10" Margin="2,59" Fill="DarkGreen" StrokeThickness="3"/>
			  <Ellipse Width="10" Height="10" Margin="59,2" Fill="DarkGreen" StrokeThickness="3"/>
			  <Ellipse Width="10" Height="10" Margin="59,118" Fill="DarkGreen" StrokeThickness="3"/>
			  <Ellipse Width="10" Height="10" Margin="118,59" Fill="DarkGreen" StrokeThickness="3"/>
			  <Grid RowDefinitions="*,*,*,*" ColumnDefinitions="*,*,*" Margin="10,10,10,10">
				  
				  <TextBlock Grid.Row="0" Grid.Column="0" Text="{TemplateBinding MyTupeText}"/>
				  <TextBlock Grid.Row="0" Grid.Column="1" Text="{TemplateBinding MyExtension}"/> 
				  <TextBlock Grid.Row="0" Grid.Column="2" Text="{TemplateBinding TupeName}"/>
				  
				  
				  <ListBox Grid.Row="2" Grid.Column="0" Items="{TemplateBinding CustomMethList}" Name="Metods">
					  <ListBox.ItemTemplate>
						  <DataTemplate>
							  <Grid ColumnDefinitions="*,*,*">
								  <TextBlock Grid.Column="0" Text="{Binding Аccess}"/>
								  <TextBlock Grid.Column="1" Text="{Binding Return}"/>
								  <TextBlock Grid.Column="2" Text="{Binding Name}"/>
							  </Grid>
						  </DataTemplate>
					  </ListBox.ItemTemplate>
				  </ListBox>
				  <ListBox Grid.Row="3" Grid.Column="0" Items="{TemplateBinding CustomProList}" Name="Propertis">
					  <ListBox.ItemTemplate>
						  <DataTemplate>
							  <Grid ColumnDefinitions="*,*">
								  <TextBlock Grid.Column="0" Text="{Binding Аccess}"/>
								  <TextBlock Grid.Column="1" Text="{Binding Return}"/>
								  <TextBlock Grid.Column="2" Text="{Binding Name}"/>
							  </Grid>
						  </DataTemplate>
					  </ListBox.ItemTemplate>
				  </ListBox>
			  </Grid>
		  </Canvas>
		 
      </ControlTemplate>
    </Setter>
  </Style>
</Styles>
